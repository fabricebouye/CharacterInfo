                <StackPane maxHeight="-1" maxWidth="-1" styleClass="portrait-box">
                    <children> 
                        <ImageView fitHeight="48.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true" />
                    </children> 
                </StackPane>
            </children>
        </Group>
        <Label id="nameLabel" fx:id="nameLabel" maxWidth="1.7976931348623157E308" styleClass="main-text" text="Text" GridPane.columnIndex="1" />
