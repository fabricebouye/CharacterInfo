<GridPane prefHeight="75.0" prefWidth="300.0" styleClass="table-view, character-info" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="test.scene.renderer.CharacterRendererController">
