<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="rootPane" fx:id="rootPane" styleClass="root-pane" prefHeight="600.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="test.controller.CharacterInfoController">
    <children>
        <TextFlow  id="applicationKeyFlow" fx:id="applicationKeyFlow" VBox.vgrow="NEVER">
            <children>    
                <Text id="applicationKeyLabel" fx:id="applicationKeyLabel" text="%application.key.label" styleClass="main-text" />
            </children>    
        </TextFlow>
        <TextField id="applicationKeyField" fx:id="applicationKeyField" />
        <TextFlow  id="accountCharacterFlow" fx:id="accountCharacterFlow" VBox.vgrow="NEVER">
            <children>    
                <Text id="accountCharacterLabel" fx:id="accountCharacterLabel" text="%no.account.label" styleClass="main-text" />
            </children>    
        </TextFlow>
        <StackPane VBox.vgrow="ALWAYS" >
            <children>
                <VBox id="listingVBox" fx:id="listingVBox" styleClass="vbox">
                    <children>
                        <TextField id="searchField" fx:id="searchField" promptText="%search.label" />
                        <ListView id="characterListView" fx:id="characterListView" VBox.vgrow="ALWAYS" />                
                    </children>
                </VBox>
                <ProgressIndicator id="progressIndicator" fx:id="progressIndicator" maxHeight="64.0" maxWidth="64.0" />
            </children>
        </StackPane>
    </children>
</VBox>
